{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Svelte Specialist Agent Output",
  "description": "Output schema for svelte-specialist agent",
  "type": "object",
  "properties": {
    "projectStructure": {
      "type": "object",
      "description": "Generated project structure",
      "properties": {
        "directories": { "type": "array", "items": { "type": "string" } },
        "entryPoint": { "type": "string" },
        "configuration": { "type": "object" }
      }
    },
    "components": {
      "type": "array",
      "description": "Generated components",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "code": { "type": "string" },
          "imports": { "type": "array", "items": { "type": "string" } }
        }
      }
    },
    "routes": {
      "type": "array",
      "description": "SvelteKit routes",
      "items": {
        "type": "object",
        "properties": {
          "path": { "type": "string" },
          "component": { "type": "string" },
          "layout": { "type": "string" }
        }
      }
    }
  },
  "required": ["projectStructure", "components"]
}
