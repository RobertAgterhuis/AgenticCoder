{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MySQL Specialist Agent Output",
  "description": "Output schema for mysql-specialist agent",
  "type": "object",
  "properties": {
    "schema": {
      "type": "object",
      "description": "Generated database schema"
    },
    "tables": {
      "type": "array",
      "description": "Table definitions",
      "items": { "type": "object" }
    },
    "indexes": {
      "type": "array",
      "description": "Index recommendations",
      "items": { "type": "object" }
    }
  },
  "required": ["schema", "tables"]
}
