{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MySQL Optimization Skill Output",
  "description": "Output schema for mysql-optimization skill",
  "type": "object",
  "properties": {
    "optimizations": {
      "type": "array",
      "items": { "type": "object" }
