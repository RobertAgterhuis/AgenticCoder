{
  "name": "@agenticcoder/agents",
  "version": "1.0.0",
  "description": "AgenticCoder agent framework and core agents",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "node scripts/run-tests.mjs",
    "test:watch": "node scripts/run-tests.mjs --watch",
    "test:azure-mcp": "node scripts/test-azure-mcp.mjs",
    "test:azure-mcp:quick": "node scripts/test-azure-mcp.mjs --quick",
    "start": "node examples/simple-workflow.js",
    "scenario": "node scripts/run-scenario.mjs",
    "scenario:s01": "node scripts/run-scenario.mjs --scenario ../test-data/S01-simple-app-service.json --out ../output/S01 --offline",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "agents",
    "workflow",
    "orchestration",
    "azure",
    "ai"
  ],
  "author": "AgenticCoder Team",
  "license": "MIT",
  "dependencies": {
    "acorn": "^8.11.3",
    "acorn-walk": "^8.3.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "glob": "^10.3.10",
    "uuid": "^9.0.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
