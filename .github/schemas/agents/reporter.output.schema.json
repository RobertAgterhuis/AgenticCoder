{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://agenticcoder.com/schemas/agents/reporter.output.schema.json",
  "type": "object",
  "title": "Reporter Agent Output",
  "description": "Generated reports and visualizations",
  "required": [
    "reports"
  ],
  "properties": {
    "reports": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "content": {
            "type": "string"
          },
          "format": {
            "type": "string"
          }
        }
      }
    }
  }
}