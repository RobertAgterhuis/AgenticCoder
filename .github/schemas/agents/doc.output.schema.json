{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://agenticcoder.com/schemas/agents/doc.output.schema.json",
  "type": "object",
  "title": "Documentation Generator Output",
  "description": "Generated documentation files and index",
  "required": [
    "documents",
    "index"
  ],
  "properties": {
    "documents": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "filename": {
            "type": "string"
          },
          "content": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        }
      }
    },
    "index": {
      "type": "object",
      "properties": {
        "sections": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "toc": {
          "type": "string"
        }
      }
    }
  }
}